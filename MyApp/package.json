{
  "name": "mobile-order-tracker",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:build": "expo export --platform web --no-minify",
    "build": "expo export --platform web --no-minify",
    "deploy": "expo export --platform web && vercel --prod",
    "preview": "expo export --platform web && npx serve dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit || echo 'No TypeScript config found'",
    "check-deps": "node scripts/check-deps.js",
    "check-styles": "node scripts/check-styles-advanced.js",
    "validate": "node scripts/validate-all.js",
    "check-styles-advanced": "node scripts/check-styles-advanced.js",
    "update-deps": "npx npm-check-updates -u",
    "check-unused": "node scripts/check-unused-files.js",
    "quick-fix": "node scripts/quick-fix.js",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/cli": "54.0.13",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@maplibre/maplibre-react-native": "^10.3.0",
    "@react-google-maps/api": "^2.20.7",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.75.0",
    "ajv": "^8.17.1",
    "expo": "54.0.19",
    "expo-application": "~7.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "^18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-manifests": "~1.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.8",
    "geolib": "^3.3.4",
    "glob": "^11.0.3",
    "maplibre-gl": "^5.9.0",
    "metro-cache": "^0.83.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-map-gl": "^8.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.82.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-preset-expo": "~11.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-import-resolver-react-native": "^0.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "expo-module-scripts": "^5.0.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "npm-check-updates": "^19.1.0",
    "patch-package": "^8.0.1",
    "react-native-svg-transformer": "^1.5.1"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "@react-navigation/native",
        "@react-navigation/bottom-tabs"
      ]
    }
  }
}
