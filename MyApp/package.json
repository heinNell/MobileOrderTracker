{
  "name": "mobile-order-tracke",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit || echo 'No TypeScript config found'",
    "check-deps": "node scripts/check-deps.js",
    "check-styles": "node scripts/check-styles-advanced.js",
    "validate": "node scripts/validate-all.js",
    "check-styles-advanced": "node scripts/check-styles-advanced.js",
    "update-deps": "npx npm-check-updates -u",
    "check-unused": "node scripts/check-unused-files.js",
    "quick-fix": "node scripts/quick-fix.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.75.0",
    "expo": "54.0.6",
    "expo-application": "~7.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.9",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-manifests": "~1.0.8",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@react-native-community/cli": "^20.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-preset-expo": "~54.0.4",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "expo-module-scripts": "^5.0.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "npm-check-updates": "^19.1.0"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "@react-navigation/native",
        "@react-navigation/bottom-tabs"
      ]
    }
  }
}
