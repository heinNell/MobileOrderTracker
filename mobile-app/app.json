{
  "expo": {
    "name": "Mobile Order Tracker",
    "slug": "mobile-order-tracker",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ordertracker",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.logistics.ordertracker",
      "associatedDomains": [
        "applinks:ordertracker.app",
        "applinks:magnificent-snickerdoodle-018e86.netlify.app"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs camera access to scan QR codes for starting and managing delivery orders.",
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) uses your location to show your position on the delivery route map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "$(PRODUCT_NAME) requires background location access to track deliveries and send geofence alerts even when the app is not active.",
        "UIBackgroundModes": ["location", "fetch", "remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.logistics.ordertracker",
            "CFBundleURLSchemes": ["ordertracker"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.logistics.ordertracker",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "ordertracker.app"
            },
            {
              "scheme": "https",
              "host": "magnificent-snickerdoodle-018e86.netlify.app"
            },
            {
              "scheme": "ordertracker"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons"]
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) needs your location to track deliveries in real-time and send geofence alerts."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "$(PRODUCT_NAME) requires camera access to scan QR codes for order verification."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563eb",
          "sounds": []
        }
      ]
    ],
    "description": "Logistics QR Code Mobile Application for Drivers - Real-time order tracking and delivery management",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/48c901a8-ab87-4a04-abfc-9a2180c9d282"
    },
    "notification": {
      "icon": "./assets/icon.png",
